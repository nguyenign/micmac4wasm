<Global
    Subst="@$#1"
    NameDecl="@$#1"

    AeroIn="-Init"
    AeroOut="-Cal120"

>
   <ParamApero>


       <SectionBDD_Observation>
             <BDD_PtsLiaisons>
                 <Id>    Id_Pastis_Hom  </Id>
                 <KeySet> Key-Set-HomolPastisBin  </KeySet>
                 <UseAsPtMultiple> true </UseAsPtMultiple>
                 <KeyAssoc>  Key-Assoc-CpleIm2HomolPastisBin   </KeyAssoc>
             </BDD_PtsLiaisons>
             <BDD_Orient>
                  <Id> Key-Ori-Init </Id>
                  <KeySet>  KeyStd-Set-Orient@${AeroIn} </KeySet>
                  <KeyAssoc>  KeyStd-Assoc-Im2Orient@${AeroIn} </KeyAssoc>
             </BDD_Orient>
       </SectionBDD_Observation>

       <SectionInconnues>

             <CalibrationCameraInc>
                   <Name> TheKeyCalib_028 </Name>
                   <CalValueInit>
                       <CalFromFileExtern>
		           <NameFile>  Ori${AeroIn}/F028_AutoCal.xml   </NameFile>
			   <NameTag>    CalibrationInternConique </NameTag>
                       </CalFromFileExtern>
                   </CalValueInit>
              </CalibrationCameraInc>

             <CalibrationCameraInc>
                   <Name> TheKeyCalib_120 </Name>
                   <CalValueInit>
                       <CalFromFileExtern>
                           <NameFile>   Calib-NoDist-PentaxK5-120Argentique.xml   </NameFile>
                           <NameTag>    CalibrationInternConique </NameTag>
                       </CalFromFileExtern>
                   </CalValueInit>
              </CalibrationCameraInc>



              <PoseCameraInc>
	           <PatternName>     F028_IMGP[0-9]{4}_MpDcraw8B_GR.tif   </PatternName>
	           <CalcNameCalib>  Key-Assoc-CalculKeyCalib </CalcNameCalib>
                   <PosValueInit>
	               <PosFromBDOrient> Key-Ori-Init </PosFromBDOrient>
                   </PosValueInit>
                   <PosesDeRattachement>  0  </PosesDeRattachement>
              </PoseCameraInc>

              <PoseCameraInc>
	           <PatternName>     F120_IMGP((2242)|(2276)|(2228)|(2285)|(2267))_MpDcraw8B_GR.tif    </PatternName> 
                   <AutoRefutDupl> true </AutoRefutDupl>

                   <InitNow> false </InitNow>
	           <CalcNameCalib>  Key-Assoc-CalculKeyCalib </CalcNameCalib>


	           <MEP_SPEC_MST> 
		           <Show> true </Show>
                           <MontageOnInit>true</MontageOnInit>
	           </MEP_SPEC_MST>


		   <PosValueInit>
		        <PoseFromLiaisons>
                             <LiaisonsInit>
                                 <NameCam> ###   </NameCam>
				 <IdBD> Id_Pastis_Hom  </IdBD>
                             </LiaisonsInit>
		        </PoseFromLiaisons>
		   </PosValueInit>
              </PoseCameraInc>


        </SectionInconnues>


        <SectionChantier>
	     <DirectoryChantier> ThisDir
             </DirectoryChantier>
        </SectionChantier>

        <SectionSolveur>
	      <ModeResolution> eSysPlein </ModeResolution> 
        </SectionSolveur>


	<SectionCompensation>
	      <EtapeCompensation>
                    <IterationsCompensation>
                       <Pose2Init>
                             <ProfMin> [2,4] </ProfMin> 
                             <Show> true </Show>
                       </Pose2Init>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberteFocale_0  </Val>
		              <Val> eLib_PP_CD_00     </Val>
		              <Val> eLiberte_DR0      </Val>
		          </ContraintesCamerasInc>
		          <ContraintesPoses>
			     <NamePose>    F028_.* </NamePose>
			     <Val>      ePoseFigee   </Val>
                             <ByPattern> true  </ByPattern>
		          </ContraintesPoses>
		       </SectionContraintes>
                    </IterationsCompensation>

                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>


		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                  <EcartMax> 200 </EcartMax>
                                  <SigmaPond> 15 </SigmaPond>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
		    </SectionObservations>
	      </EtapeCompensation>

	      <EtapeCompensation>

                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberte_DR3      </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>


                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberteFocale_1  </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>

                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLib_PP_CD_01  </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>

                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLib_PP_CD_11  </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>

                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesPoses>
			     <NamePose>   .* </NamePose>
			     <Val>      ePoseLibre   </Val>
                             <ByPattern> true  </ByPattern>
		          </ContraintesPoses>
		          <ContraintesPoses>
			     <NamePose>   0 </NamePose>
			     <Val>      ePoseFigee   </Val>
		          </ContraintesPoses>
		          <ContraintesPoses>
			     <NamePose>   1     </NamePose>
			     <Val>      ePoseBaseNormee   </Val>
			     <PoseRattachement>   0 </PoseRattachement>
		          </ContraintesPoses>
		       </SectionContraintes>
                    </IterationsCompensation>


                    <IterationsCompensation> </IterationsCompensation>




		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                  <EcartMax> 10 </EcartMax>
                                  <SigmaPond> 4 </SigmaPond>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
		    </SectionObservations>


	      </EtapeCompensation>

	      <EtapeCompensation>

                    <IterationsCompensation>
                    </IterationsCompensation>

                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>

		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
				 <ModePonderation>  eL1Secured </ModePonderation>
				 <SigmaPond> 2.0 </SigmaPond>
				 <EcartMax> 5.0 </EcartMax>
                              </Pond>
                           </ObsLiaisons>
		    </SectionObservations>

                    <SectionExport>
                        <ExportPose>
                            <PatternSel> (.*).tif </PatternSel>
                            <KeyAssoc> KeyStd-Assoc-Im2Orient@${AeroOut}  </KeyAssoc>
                            <AddCalib>  true </AddCalib>
			    <NbVerif>  10 </NbVerif>
			    <TolWhenVerif> 1e-3 </TolWhenVerif>
                            <FileExtern> KeyStd-Assoc-Im2AutoCal@${AeroOut} </FileExtern>
                            <FileExternIsKey> true </FileExternIsKey>
                        </ExportPose>
                        <ExportCalib>
                             <KeyAssoc>  KeyStd-Assoc-CleCal2AutoCal@${AeroOut} </KeyAssoc>
                             <KeyIsName> false </KeyIsName>
                        </ExportCalib>

                    </SectionExport>

	      </EtapeCompensation>
	</SectionCompensation>

   </ParamApero>
</Global>

