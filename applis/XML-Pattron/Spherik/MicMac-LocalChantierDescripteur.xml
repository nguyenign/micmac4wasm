<!--


  843   3479   ;  Ec = 2636

  Ec / Pi = 839 (= Focale en pixel)

   2868 Pix = 16 mm

   Foc = 16 * 839/2868  = 4.68 mm

 Calcul donne 1009.6

       1009.6 / 839   * 4.7 = 5.65
-->

<Global>

  <ChantierDescripteur>
        <KeyedNamesAssociations>
            <Calcs>
                 <Arrite>  1 1 </Arrite>
                 <Direct>
                       <PatternTransform> .*DSC_[0-9]{4}.NEF  </PatternTransform>
                       <CalcName> 5.65 </CalcName>
                 </Direct>
             </Calcs>
             <Key>   NKS-Assoc-STD-FOC </Key>
        </KeyedNamesAssociations>
<!--
-->
  </ChantierDescripteur>
</Global>

