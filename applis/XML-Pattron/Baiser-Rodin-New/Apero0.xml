<Global
    Subst="@$#1"
    NameDecl="@$#1"

    AeroOut="-Init"
>
   <ParamApero>


       <SectionBDD_Observation>
             <BDD_PtsLiaisons>
                 <Id>    Id_Pastis_Hom  </Id>
                 <KeySet> Key-Set-HomolPastisBin  </KeySet>
                 <UseAsPtMultiple> true </UseAsPtMultiple>
                 <KeyAssoc>  Key-Assoc-CpleIm2HomolPastisBin   </KeyAssoc>
             </BDD_PtsLiaisons>

       </SectionBDD_Observation>

       <SectionInconnues>

             <CalibrationCameraInc>
                   <Name> TheKeyCalib_350 </Name>
                   <CalValueInit>
                       <CalFromFileExtern>
		           <NameFile>   CalibGeomInit_MatisD5MarkII_350.xml    </NameFile>
			   <NameTag>    CalibrationInternConique </NameTag>
                       </CalFromFileExtern>
                   </CalValueInit>
              </CalibrationCameraInc>

             <PoseCameraInc>
	           <PatternName>    ${PrefixImages}0071${PostfixImagesDebay}    </PatternName>
	           <CalcNameCalib>  KeyStd-Assoc-KeyCalib@ </CalcNameCalib>
                   <PosValueInit>
	               <PosId> ### </PosId>
                   </PosValueInit>
             </PoseCameraInc>

              <PoseCameraInc>
	           <PatternName>     ${PatternAllIm}  </PatternName> 
                   <PatternRefuteur> ${PrefixImages}010[7-8]${PostfixImagesDebay}   </PatternRefuteur>
<!--
-->
                   <Filter>
                        <DataBaseFilter>
                             <FocMm> 35.0  35.0 </FocMm>
                         </DataBaseFilter>
                   </Filter>

                   <InitNow> false </InitNow>
	           <CalcNameCalib>  KeyStd-Assoc-KeyCalib@ </CalcNameCalib>

                   <PosesDeRattachement>  0  </PosesDeRattachement>

                   <MEP_SPEC_MST> 
                           <Show> true </Show>
                   </MEP_SPEC_MST>
		   <PosValueInit>
		        <PoseFromLiaisons>
                             <LiaisonsInit>
                                 <NameCam> ###   </NameCam>
				 <IdBD> Id_Pastis_Hom  </IdBD>
                             </LiaisonsInit>
		        </PoseFromLiaisons>
		   </PosValueInit>
              </PoseCameraInc>



        </SectionInconnues>


        <SectionChantier>
	     <DirectoryChantier> ThisDir
             </DirectoryChantier>
        </SectionChantier>

        <SectionSolveur>
              <AllMatSym>  true </AllMatSym>
	      <ModeResolution> eSysPlein </ModeResolution> 
<!--
              <ModeResolution> eSysCreuxMap </ModeResolution> 
-->
              <GridOptimKnownDist> true </GridOptimKnownDist>
              <NormaliseEqSc> true </NormaliseEqSc>
              <NormaliseEqTr> true </NormaliseEqTr>
        </SectionSolveur>


	<SectionCompensation>
	      <EtapeCompensation>
                    <IterationsCompensation>
                       <Pose2Init>
                             <ProfMin> [2,4] </ProfMin> 
                             <Show> true </Show>
                       </Pose2Init>
		       <SectionContraintes>

		          <ContraintesCamerasInc>
		              <Val> eLiberteFocale_0  </Val>
		              <Val> eLib_PP_CD_00     </Val>
		              <Val> eLiberte_DR0      </Val>

		          </ContraintesCamerasInc>

		          <ContraintesPoses>
			     <NamePose>   0 </NamePose>
			     <Val>      ePoseFigee   </Val>
		          </ContraintesPoses>

		          <ContraintesPoses>
			     <NamePose>   1     </NamePose>
			     <Val>      ePoseBaseNormee   </Val>
<!--
			     <Val>      ePoseVraieBaseNormee   </Val>
			     <Val>      ePoseBaseNormee   </Val>
-->
			     <PoseRattachement>   0 </PoseRattachement>
		          </ContraintesPoses>
		       </SectionContraintes>
                    </IterationsCompensation>

                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>


		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                  <EcartMax> 100 </EcartMax>
                                  <SigmaPond> 5 </SigmaPond>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
		    </SectionObservations>

	      </EtapeCompensation>

	      <EtapeCompensation>


                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberteFocale_1  </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>

                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberte_DR3      </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>

                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLib_PP_CD_11      </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>

		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                  <EcartMax> 10 </EcartMax>
                                  <SigmaPond> 4 </SigmaPond>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
		    </SectionObservations>


	      </EtapeCompensation>

	      <EtapeCompensation>

                    <IterationsCompensation>
                    </IterationsCompensation>
                   <IterationsCompensation>
                    </IterationsCompensation>


                    <IterationsCompensation> </IterationsCompensation>

		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
				 <ModePonderation>  eL1Secured </ModePonderation>
				 <SigmaPond> 2.0 </SigmaPond>
				 <EcartMax> 5.0 </EcartMax>
                              </Pond>
                           </ObsLiaisons>
		    </SectionObservations>

                    <SectionExport>
                            <ExportPose>
                                <PatternSel> (.*).tif </PatternSel>
                                <KeyAssoc> KeyStd-Assoc-Im2Orient@${AeroOut} </KeyAssoc>
                                <AddCalib>  true </AddCalib>
                                <NbVerif>  10 </NbVerif>
                                <TolWhenVerif> 1e-3 </TolWhenVerif>

                                <FileExtern>  KeyStd-Assoc-CleCal2AutoCal@${AeroOut} </FileExtern>
                                <CalcKeyFromCalib> true </CalcKeyFromCalib>
                                <ModeAngulaire> true </ModeAngulaire>
                            </ExportPose>
                            <ExportCalib>
                                 <KeyAssoc>  KeyStd-Assoc-CleCal2AutoCal@${AeroOut} </KeyAssoc>
                                 <KeyIsName> false </KeyIsName>
                            </ExportCalib>

                    </SectionExport>

	      </EtapeCompensation>
	</SectionCompensation>

   </ParamApero>
</Global>

