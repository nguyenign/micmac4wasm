<Global
    Subst="@$#1"
    NameDecl="@$#1"

    NumC="05042"
    Pattern="(([0-9]{5})|(0504[0-4])|(0483[0-4])|(0525[2-6]))"

    AeroIn="-A0-Navig-UTM"
    AeroOut="-NewA1-NavLiais-UTM"
    ZMoy="-6.0"
>
   <ParamApero>


       <SectionBDD_Observation>
             <BDD_PtsLiaisons>
                 <Id>    Id_Pastis_Hom  </Id>
                 <KeySet> Key-Set-HomolPastisBin  </KeySet>
                 <UseAsPtMultiple> true </UseAsPtMultiple>
                 <KeyAssoc>  Key-Assoc-CpleIm2HomolPastisBin   </KeyAssoc>
             </BDD_PtsLiaisons>

            <BDD_Centre>
                  <Id > Id-Centre </Id>
                  <KeySet>  KeyStd-Set-Orient@${AeroIn} </KeySet>
                  <KeyAssoc>  KeyStd-Assoc-Im2Orient@${AeroIn} </KeyAssoc>
            </BDD_Centre>

            <BDD_Orient>
                  <Id>  Id-Or-Nav</Id>
                  <KeySet>  KeyStd-Set-Orient@${AeroIn} </KeySet>
                  <KeyAssoc>  KeyStd-Assoc-Im2Orient@${AeroIn} </KeyAssoc>
            </BDD_Orient>



       </SectionBDD_Observation>

       <SectionInconnues>

             <CalibrationCameraInc>
                   <Name> TheKeyCalib_${Focale} </Name>
                   <CalValueInit>
                       <CalFromFileExtern>
		           <NameFile>  Calib-Sony-${Focale}-${TypeCal}.xml    </NameFile>
			   <NameTag>    CalibrationInternConique </NameTag>
                       </CalFromFileExtern>
                   </CalValueInit>
              </CalibrationCameraInc>

              <PoseCameraInc>
                   <PatternName>    Key-Set-All-Nav-Im   </PatternName>                                   
                   <ByKey>          true                      </ByKey>

	           <CalcNameCalib>  Key-Assoc-CalculKeyCalib </CalcNameCalib>
                   <IdBDCentre> Id-Centre </IdBDCentre>

                   <PosValueInit>
	               <PosFromBDOrient> Id-Or-Nav </PosFromBDOrient>
                   </PosValueInit>
              </PoseCameraInc>


        </SectionInconnues>


        <SectionChantier>
	     <DirectoryChantier> ThisDir
             </DirectoryChantier>
             <DefPbLiaison >
                 <NbMinPtsMul> 100 </NbMinPtsMul>
                 <Actif >  true </Actif> 
                 <GetCharOnPb>  false </GetCharOnPb>
             </DefPbLiaison>
        </SectionChantier>

        <SectionSolveur>
              <ModeControleDescDic> eCDD_Jamais </ModeControleDescDic>
              <AllMatSym>  true </AllMatSym>
	      <ModeResolution> eSysL2BlocSym </ModeResolution> 
              <GridOptimKnownDist> true </GridOptimKnownDist>
              <NormaliseEqSc> true </NormaliseEqSc>
              <NormaliseEqTr> true </NormaliseEqTr>
        </SectionSolveur>


	<SectionCompensation>
	      <EtapeCompensation>
                    <IterationsCompensation>
		       <SectionContraintes>

		          <ContraintesCamerasInc>
		              <Val> eLiberteFocale_0  </Val>
		              <Val> eLib_PP_CD_00     </Val>
		              <Val> eLiberte_DR0      </Val>
		              <Val> eFige_Phgr_Std_Aff      </Val>
		              <Val> eFige_Phgr_Std_Dec      </Val>
		          </ContraintesCamerasInc>

		       </SectionContraintes>
                       <SLMIter>
                          <RappelOnAngles>
                             <ParamF> 
                                  <PatternNameApply>  .*</PatternNameApply>
                                  <Incertitude> 0.1 </Incertitude>
                                  <OnCur>  true </OnCur>  
                             </ParamF>
                             <TetaApply>  1</TetaApply>
                             <TetaApply>  2</TetaApply>
                          </RappelOnAngles>
                       </SLMIter>

                       <SLMGlob>
                          <RappelOnAngles>
                             <ParamF> 
                                  <PatternNameApply>  .*</PatternNameApply>
                                  <Incertitude> 10 </Incertitude>
                                  <OnCur>  true </OnCur>  
                             </ParamF>
                             <TetaApply>  0</TetaApply>
                          </RappelOnAngles>

                          <RappelOnCentres>
                             <ParamF> 
                                  <PatternNameApply>  .*</PatternNameApply>
                                  <Incertitude> 100 </Incertitude>
                                  <OnCur>  true </OnCur>  
                             </ParamF>
                             <OnlyWhenNoCentreInit> true </OnlyWhenNoCentreInit>
                          </RappelOnCentres>

                       </SLMGlob>

                    </IterationsCompensation>

                    <IterationsCompensation> <MultSLMIter> 0.5  </MultSLMIter> </IterationsCompensation>
                    <IterationsCompensation> <MultSLMIter> 0.2  </MultSLMIter> </IterationsCompensation>
                    <IterationsCompensation> <MultSLMIter> 0.1  </MultSLMIter> </IterationsCompensation>
                    <IterationsCompensation> <MultSLMIter> 0.05  </MultSLMIter> </IterationsCompensation>

		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <RappelOnZ  Z="-6.0" IncC="10.0"/>
                              <Pond>
                                 <EcartMesureIndiv>  10000.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                  <EcartMax> 4000 </EcartMax>
                                  <SigmaPond> 500 </SigmaPond>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
                           <ObsCentrePDV >
                              <PatternApply> .* </PatternApply>
                              <Pond>
                                  <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                  <Show> eNSM_Paquet     </Show>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsCentrePDV>
		    </SectionObservations>

	      </EtapeCompensation>

	      <EtapeCompensation>
                    <IterationsCompensation> <MultSLMIter> 0.02  </MultSLMIter> </IterationsCompensation>
                    <IterationsCompensation> <MultSLMIter> 0.01  </MultSLMIter> </IterationsCompensation>
		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <RappelOnZ  Z="-6.0" IncC="30.0"/>
                              <Pond>
                                 <EcartMesureIndiv>  200.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                  <EcartMax> 500 </EcartMax>
                                  <SigmaPond> 50 </SigmaPond>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
                           <ObsCentrePDV >
                              <PatternApply> .* </PatternApply>
                              <Pond>
                                  <EcartMesureIndiv>  2.0 </EcartMesureIndiv>
                                  <Show> eNSM_Paquet     </Show>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsCentrePDV>
		    </SectionObservations>
	      </EtapeCompensation>
	      <EtapeCompensation>
                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>
		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <RappelOnZ  Z="-6.0" IncC="100.0"/>
                              <Pond>
                                 <EcartMesureIndiv>  10.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                  <EcartMax> 100 </EcartMax>
                                  <SigmaPond> 25 </SigmaPond>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
                           <ObsCentrePDV >
                              <PatternApply> .* </PatternApply>
                              <Pond>
                                  <EcartMesureIndiv>  2.0 </EcartMesureIndiv>
                                  <Show> eNSM_Paquet     </Show>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsCentrePDV>
		    </SectionObservations>
	      </EtapeCompensation>

	      <EtapeCompensation>
                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>
		    <SectionObservations>
                           <ObsLiaisons>
                              <RappelOnZ  Z="-6.0" IncC="500.0"/>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                  <EcartMax> 50 </EcartMax>
                                  <SigmaPond> 10 </SigmaPond>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
                           <ObsCentrePDV >
                              <PatternApply> .* </PatternApply>
                              <Pond>
                                  <EcartMesureIndiv>  2.0 </EcartMesureIndiv>
                                  <Show> eNSM_Paquet     </Show>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsCentrePDV>
		    </SectionObservations>
	      </EtapeCompensation>




	      <EtapeCompensation>

                    <IterationsCompensation>
                       <Messages> ------------DR1 --------------------</Messages>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberte_DR1      </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation>
                       <Messages> ------------DR2 --------------------</Messages>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberte_DR2      </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation>
                       <Messages> ------------DR3 --------------------</Messages>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberte_DR3      </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>

                   <IterationsCompensation>
                       <Messages> ------------PP CD --------------------</Messages>
                       <SectionContraintes>
                          <ContraintesCamerasInc>
                              <Val> eLib_PP_CD_01      </Val>
                          </ContraintesCamerasInc>
                       </SectionContraintes>
                    </IterationsCompensation>

                    <IterationsCompensation>
                       <Messages> ------------ DECENTRIK --------------------</Messages>
                       <SectionContraintes>
                          <ContraintesCamerasInc>
                              <Val> eLiberte_Phgr_Std_Dec      </Val>
                          </ContraintesCamerasInc>
                       </SectionContraintes>
                    </IterationsCompensation>

                   <IterationsCompensation>
                       <Messages> ------------PP CD --------------------</Messages>
                       <SectionContraintes>
                          <ContraintesCamerasInc>
                              <Val> eLib_PP_CD_11      </Val>
                          </ContraintesCamerasInc>
                       </SectionContraintes>
                    </IterationsCompensation>


                    <IterationsCompensation> </IterationsCompensation>

                    <IterationsCompensation>
                       <Messages> ------------ FOC --------------------</Messages>
                       <SectionContraintes>
                          <ContraintesCamerasInc>
                              <Val> eLiberteFocale_1      </Val>
                          </ContraintesCamerasInc>
                       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>


                    <IterationsCompensation>
                       <Messages> ------------ AFFINES --------------------</Messages>
                       <SectionContraintes>
                          <ContraintesCamerasInc>
                              <Val> eLiberte_Phgr_Std_Aff      </Val>
                          </ContraintesCamerasInc>
                       </SectionContraintes>
                    </IterationsCompensation>




                    <IterationsCompensation> </IterationsCompensation>



		    <SectionObservations>
                           <ObsLiaisons>
                              <RappelOnZ  Z="-6.0" IncC="500.0"/>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                  <EcartMax> 25 </EcartMax>
                                  <SigmaPond> 8 </SigmaPond>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
                           <ObsCentrePDV >
                              <PatternApply> .* </PatternApply>
                              <Pond>
                                  <EcartMesureIndiv>  2.0 </EcartMesureIndiv>
                                  <Show> eNSM_Paquet     </Show>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsCentrePDV>
		    </SectionObservations>

	      </EtapeCompensation>
	      <EtapeCompensation>

                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>

		    <SectionObservations>
                           <ObsLiaisons>
                              <RappelOnZ  Z="-6.0" IncC="100.0"/>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                 <EcartMax>  10 </EcartMax>
                                 <SigmaPond> 2 </SigmaPond>
                                 <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
                           <ObsCentrePDV >
                              <PatternApply> .* </PatternApply>
                              <Pond>
                                  <EcartMesureIndiv>  2.0 </EcartMesureIndiv>
                                  <Show> eNSM_Paquet     </Show>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsCentrePDV>
		    </SectionObservations>
                    <SectionExport>
                        <ExportPose>
                            <PatternSel> (.*).tif </PatternSel>
                            <KeyAssoc> KeyStd-Assoc-Im2Orient@${AeroOut} </KeyAssoc>
                            <AddCalib>  true </AddCalib>
			    <NbVerif>  10 </NbVerif>
			    <TolWhenVerif> 1e-3 </TolWhenVerif>
                            <FileExtern> KeyStd-Assoc-Im2AutoCal@${AeroOut} </FileExtern>
                            <FileExternIsKey> true </FileExternIsKey>
                            <ModeAngulaire> true </ModeAngulaire>
                        </ExportPose>
                        <ExportCalib>
                             <KeyAssoc>  KeyStd-Assoc-CleCal2AutoCal@${AeroOut} </KeyAssoc>
                             <KeyIsName> false </KeyIsName>
                        </ExportCalib>

                    </SectionExport>

	      </EtapeCompensation>

	</SectionCompensation>

   </ParamApero>
</Global>

