<Global
    Subst="@$#1"
    NameDecl="@$#1"

    AeroOut="-Glob"
>
   <ParamApero>


       <SectionBDD_Observation>
             <BDD_PtsLiaisons>
                 <Id>    Id_Pastis_Hom  </Id>
                 <KeySet> Key-Set-HomolPastisBin  </KeySet>
                 <UseAsPtMultiple> true </UseAsPtMultiple>
                 <KeyAssoc>  Key-Assoc-CpleIm2HomolPastisBin   </KeyAssoc>
             </BDD_PtsLiaisons>

             <BDD_Orient>
                  <Id>  IdOrInit</Id>
                  <KeySet> Key-Set-All-OrInit </KeySet>
                  <KeyAssoc>  Key-Assoc-Im2OrInit </KeyAssoc>
             </BDD_Orient>

       </SectionBDD_Observation>

       <SectionInconnues>

             <CalibrationCameraInc>
                   <Name> TheKeyCalib_${FocGA} </Name>
                   <CalValueInit>
                       <CalFromFileExtern>
		           <NameFile>   Orient/F${FocGA}_AutoCalInit.xml     </NameFile>
			   <NameTag>    CalibrationInternConique </NameTag>
                       </CalFromFileExtern>
                   </CalValueInit>
             </CalibrationCameraInc>

             <CalibrationCameraInc>
                   <Name> TheKeyCalib_016 </Name>
                   <CalValueInit>
                       <CalFromFileExtern>
		           <NameFile>   AutoCalDRad-Canon-016.xml</NameFile>
			   <NameTag>    CalibrationInternConique </NameTag>
                       </CalFromFileExtern>
                   </CalValueInit>
              </CalibrationCameraInc>
             <CalibrationCameraInc>
                   <Name> TheKeyCalib_018 </Name>
                   <CalValueInit>
                       <CalFromFileExtern>
		           <NameFile>   AutoCalDRad-Canon-018.xml</NameFile>
			   <NameTag>    CalibrationInternConique </NameTag>
                       </CalFromFileExtern>
                   </CalValueInit>
              </CalibrationCameraInc>
             <CalibrationCameraInc>
                   <Name> TheKeyCalib_024 </Name>
                   <CalValueInit>
                       <CalFromFileExtern>
		           <NameFile>   AutoCalDRad-Canon-024.xml</NameFile>
			   <NameTag>    CalibrationInternConique </NameTag>
                       </CalFromFileExtern>
                   </CalValueInit>
              </CalibrationCameraInc>



              <PoseCameraInc>
	           <PatternName>    F${FocGA}_${PrefixImages}${PatNum}${PostfixImagesDebay}.tif </PatternName>
	           <CalcNameCalib>  Key-Assoc-CalculKeyCalib </CalcNameCalib>
                   <PosesDeRattachement>  0  </PosesDeRattachement>
                   <PosValueInit>
                       <PosFromBDOrient> IdOrInit </PosFromBDOrient>
                   </PosValueInit>
              </PoseCameraInc>


              <PoseCameraInc>
	           <PatternName>    F${PatLFoc}_${PrefixImages}${PatNum}${PostfixImagesDebay}.tif </PatternName>
                   <InitNow> false </InitNow>
	           <CalcNameCalib>  Key-Assoc-CalculKeyCalib </CalcNameCalib>

                   <MEP_SPEC_MST> 
                           <Show> true </Show>
                           <MontageOnInit> true </MontageOnInit>
                   </MEP_SPEC_MST>
		   <PosValueInit>
		        <PoseFromLiaisons>
                             <LiaisonsInit>
                                 <NameCam> ###   </NameCam>
				 <IdBD> Id_Pastis_Hom  </IdBD>
                             </LiaisonsInit>
		        </PoseFromLiaisons>
		   </PosValueInit>
              </PoseCameraInc>





        </SectionInconnues>


        <SectionChantier>
	     <DirectoryChantier> ThisDir
             </DirectoryChantier>
        </SectionChantier>

        <SectionSolveur>
              <AllMatSym>  true </AllMatSym>
	      <ModeResolution> eSysPlein </ModeResolution> 
<!--
              <ModeResolution> eSysCreuxMap </ModeResolution> 
-->
              <GridOptimKnownDist> true </GridOptimKnownDist>
              <NormaliseEqSc> true </NormaliseEqSc>
              <NormaliseEqTr> true </NormaliseEqTr>
        </SectionSolveur>


	<SectionCompensation>
	      <EtapeCompensation>
                    <IterationsCompensation>
                       <Pose2Init>
                             <ProfMin> [2,4] </ProfMin> 
                             <Show> true </Show>
                       </Pose2Init>
		       <SectionContraintes>

		          <ContraintesCamerasInc>
		              <Val> eLiberteFocale_0  </Val>
		              <Val> eLib_PP_CD_00     </Val>
		              <Val> eLiberte_DR0      </Val>
		          </ContraintesCamerasInc>

		          <ContraintesPoses>
			     <NamePose>   F${FocGA}.* </NamePose>
			     <Val>      ePoseFigee   </Val>
                             <ByPattern> true </ByPattern>
		          </ContraintesPoses>

		       </SectionContraintes>
                    </IterationsCompensation>

                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>


		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                  <EcartMax> 100 </EcartMax>
                                  <SigmaPond> 5 </SigmaPond>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
		    </SectionObservations>

	      </EtapeCompensation>

	      <EtapeCompensation>

                    <IterationsCompensation> </IterationsCompensation>

                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberte_DR1      </Val>
                              <PatternNameApply> TheKeyCalib_${PatLFoc} </PatternNameApply>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberte_DR2      </Val>
                              <PatternNameApply> TheKeyCalib_${PatLFoc} </PatternNameApply>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberte_DR3      </Val>
                              <PatternNameApply> TheKeyCalib_${PatLFoc} </PatternNameApply>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>


                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberteFocale_1  </Val>
                              <PatternNameApply> TheKeyCalib_${PatLFoc} </PatternNameApply>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                       <Messages> eLiberteFocale_1 </Messages>
                    </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>


                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLib_PP_CD_01      </Val>
                              <PatternNameApply> TheKeyCalib_${PatLFoc} </PatternNameApply>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                       <Messages> eLib_PP_CD_01 </Messages>
                    </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>

                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLib_PP_CD_11      </Val>
                              <PatternNameApply> TheKeyCalib_${PatLFoc} </PatternNameApply>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                       <Messages> eLib_PP_CD_11 </Messages>
                    </IterationsCompensation>

                    <IterationsCompensation> </IterationsCompensation>

		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                  <EcartMax> 10 </EcartMax>
                                  <SigmaPond> 4 </SigmaPond>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
		    </SectionObservations>


	      </EtapeCompensation>

	      <EtapeCompensation>

                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLib_PP_CD_11      </Val>
		              <Val> eLiberteFocale_1  </Val>
		              <Val> eLiberte_DR3      </Val>
		          </ContraintesCamerasInc>

		          <ContraintesPoses>
			     <NamePose>   F${FocGA}.* </NamePose>
			     <Val>      ePoseLibre   </Val>
                             <ByPattern> true </ByPattern>
		          </ContraintesPoses>
                          <ContraintesPoses>
                             <NamePose>   0 </NamePose>
                             <Val>      ePoseFigee   </Val>
                          </ContraintesPoses>
                          <ContraintesPoses>
                             <NamePose>   1     </NamePose>
                             <Val>      ePoseBaseNormee   </Val>
                             <PoseRattachement>   0 </PoseRattachement>
                          </ContraintesPoses>
		       </SectionContraintes>
                    </IterationsCompensation>

                   <IterationsCompensation> </IterationsCompensation>
                   <IterationsCompensation> </IterationsCompensation>

