<Global
    Subst="@$#1"
>
   <ParamApero>


       <SectionBDD_Observation>
             <BDD_PtsLiaisons>
                 <Id>    Id_Pastis_Hom  </Id>
                 <KeySet> Key-Set-HomolPastisBin  </KeySet>
                 <UseAsPtMultiple> true </UseAsPtMultiple>
                 <KeyAssoc>  Key-Assoc-CpleIm2HomolPastisBin   </KeyAssoc>
             </BDD_PtsLiaisons>

       </SectionBDD_Observation>

       <SectionInconnues>

             <CalibrationCameraInc>
                   <Name> TheKeyCalib_${FocGA} </Name>
                   <CalValueInit>
                       <CalFromFileExtern>
		           <NameFile>   AutoCalDRad-Canon-${FocGA}.xml    </NameFile>
			   <NameTag>    CalibrationInternConique </NameTag>
                       </CalFromFileExtern>
                   </CalValueInit>
              </CalibrationCameraInc>

              <PoseCameraInc>
	           <PatternName>    F${FocGA}_${PrefixImages}${ImC}${PostfixImagesDebay}.tif </PatternName>
	           <CalcNameCalib>  Key-Assoc-CalculKeyCalib </CalcNameCalib>
                   <PosValueInit>
	               <PosId> ### </PosId>
                   </PosValueInit>
              </PoseCameraInc>

              <PoseCameraInc>
	           <PatternName>   F${FocGA}_${PrefixImages}${PatNum}${PostfixImagesDebay}.tif  </PatternName> 
                   <InitNow> false </InitNow>
	           <CalcNameCalib>  Key-Assoc-CalculKeyCalib </CalcNameCalib>

                   <PosesDeRattachement>  0  </PosesDeRattachement>

                   <MEP_SPEC_MST> 
                           <Show> true </Show>
                   </MEP_SPEC_MST>
		   <PosValueInit>
		        <PoseFromLiaisons>
                             <LiaisonsInit>
                                 <NameCam> ###   </NameCam>
				 <IdBD> Id_Pastis_Hom  </IdBD>
                             </LiaisonsInit>
		        </PoseFromLiaisons>
		   </PosValueInit>
              </PoseCameraInc>

        </SectionInconnues>


        <SectionChantier>
	     <DirectoryChantier> ThisDir
             </DirectoryChantier>
        </SectionChantier>

        <SectionSolveur>
              <AllMatSym>  true </AllMatSym>
	      <ModeResolution> eSysL2BlocSym </ModeResolution> 
<!--
              <ModeResolution> eSysCreuxMap </ModeResolution> 
-->
              <GridOptimKnownDist> true </GridOptimKnownDist>
              <NormaliseEqSc> true </NormaliseEqSc>
              <NormaliseEqTr> true </NormaliseEqTr>
        </SectionSolveur>


	<SectionCompensation>
	      <EtapeCompensation>
                    <IterationsCompensation>
                       <Pose2Init>
                             <ProfMin> [2,4] </ProfMin> 
                             <Show> true </Show>
                       </Pose2Init>
		       <SectionContraintes>

		          <ContraintesCamerasInc>
		              <Val> eLiberteFocale_0  </Val>
		              <Val> eLib_PP_CD_00     </Val>
		              <Val> eLiberte_DR0      </Val>

		          </ContraintesCamerasInc>

		          <ContraintesPoses>
			     <NamePose>   0 </NamePose>
			     <Val>      ePoseFigee   </Val>
		          </ContraintesPoses>

		          <ContraintesPoses>
			     <NamePose>   1     </NamePose>
			     <Val>      ePoseBaseNormee   </Val>
			     <PoseRattachement>   0 </PoseRattachement>
		          </ContraintesPoses>
		       </SectionContraintes>
                    </IterationsCompensation>

                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>


		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                  <EcartMax> 100 </EcartMax>
                                  <SigmaPond> 5 </SigmaPond>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
		    </SectionObservations>

	      </EtapeCompensation>

	      <EtapeCompensation>

                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberte_DR1      </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberte_DR2      </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberte_DR3      </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>

                    <IterationsCompensation> </IterationsCompensation>

                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLiberteFocale_1  </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>


                    <IterationsCompensation>
		       <SectionContraintes>
		          <ContraintesCamerasInc>
		              <Val> eLib_PP_CD_11      </Val>
		          </ContraintesCamerasInc>
		       </SectionContraintes>
                    </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>
                    <IterationsCompensation> </IterationsCompensation>

		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
                                  <EcartMax> 10 </EcartMax>
                                  <SigmaPond> 4 </SigmaPond>
                                  <ModePonderation> eL1Secured </ModePonderation>
                              </Pond>
                           </ObsLiaisons>
		    </SectionObservations>


	      </EtapeCompensation>

	      <EtapeCompensation>

                    <IterationsCompensation>
                    </IterationsCompensation>
                   <IterationsCompensation>
                       <BasculeOrientation >
                               <PatternNameEstim>  F${FocGA}_${PrefixImages}${PatPlan}${PostfixImagesDebay}.tif   </PatternNameEstim>
                               <ModeBascule>
                                   <BasculeLiaisonOnPlan >
                                       <EstimPl>
                                           <AttrSup>  Plan </AttrSup>
                                           <KeyCalculMasq>  Key-Assoc-Std-MultiMasq-Image </KeyCalculMasq>
                                           <IdBdl >Id_Pastis_Hom </IdBdl>
                                           <Pond>
                                              <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                              <Show> eNSM_Paquet     </Show>
                                              <NbMax>   100    </NbMax>
                                              <ModePonderation>  eL1Secured </ModePonderation>
                                              <SigmaPond> 15.0 </SigmaPond>
                                              <EcartMax> 50.0 </EcartMax>
                                       </Pond>
                                       </EstimPl>
                                   </BasculeLiaisonOnPlan>
                               </ModeBascule>
                        </BasculeOrientation>
                        <FixeOrientPlane>
                              <ModeFOP>
                                  <HorFOP>
                                     <VecFOH>
                                        <Pt>  ${PtDr1}  </Pt>
                                        <Im> F${FocGA}_${PrefixImages}${ImPl1}${PostfixImagesDebay}.tif </Im>
                                     </VecFOH>
                                     <VecFOH>
                                        <Pt>  ${PtDr2}  </Pt>
                                        <Im> F${FocGA}_${PrefixImages}${ImPl2}${PostfixImagesDebay}.tif </Im>
                                     </VecFOH>
                                  </HorFOP>
                              </ModeFOP>
                              <Vecteur>  1 0 </Vecteur>
                          </FixeOrientPlane>
                    </IterationsCompensation>
<!--
-->

                    <IterationsCompensation> </IterationsCompensation>
		    <SectionObservations>
                           <ObsLiaisons>
                              <NameRef> Id_Pastis_Hom </NameRef>
                              <Pond>
                                 <EcartMesureIndiv>  1.0 </EcartMesureIndiv>
                                 <Show> eNSM_Paquet     </Show>
                                 <NbMax>   100    </NbMax>
				 <ModePonderation>  eL1Secured </ModePonderation>
				 <SigmaPond> 2.0 </SigmaPond>
				 <EcartMax> 5.0 </EcartMax>
                              </Pond>
                           </ObsLiaisons>
		    </SectionObservations>

                    <SectionExport>
                        <ExportPose>
                            <PatternSel> (.*).tif </PatternSel>
                            <KeyAssoc> Key-Assoc-Im2OrInit </KeyAssoc>
                            <AddCalib>  true </AddCalib>
			    <NbVerif>  10 </NbVerif>
			    <TolWhenVerif> 1e-3 </TolWhenVerif>
                            <FileExtern> Key-Assoc-Im2AutoCalInit </FileExtern>
                            <FileExternIsKey> true </FileExternIsKey>
                        </ExportPose>
                        <ExportCalib>
                             <KeyAssoc>  Key-Assoc-CleCal2AutoCalInit </KeyAssoc>
                             <KeyIsName> false </KeyIsName>
                        </ExportCalib>

                    </SectionExport>

	      </EtapeCompensation>
	</SectionCompensation>

   </ParamApero>
</Global>

